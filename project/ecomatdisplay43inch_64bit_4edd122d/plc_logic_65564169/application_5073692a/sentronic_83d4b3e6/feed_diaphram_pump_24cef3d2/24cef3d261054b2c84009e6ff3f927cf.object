{"payload":{"meta":{"Graph":{"@Type":"81297157","@Value":{"Guid":"(Guid)24cef3d2-6105-4b2c-8400-9e6ff3f927cf","ParentGuid":"(Guid)5073692a-71a6-4172-b2a5-0488938c3f5f","Name":"(string)Feed_Diaphram_Pump","Properties":{"@Type":"2c41fa04:IDictionary","@Value":[{"@Key":"(Guid)829a18f2-c514-4f6e-9634-1df173429203","@Value":{"@Type":"829a18f2","@Value":{"ParentObjects":{"@Type":"fa2ee218:IDictionary","@Value":[{"@Key":"(Guid)d9b2b2cc-ea99-4c3b-aa42-1e5c49e65b84","@Value":"(Guid)83d4b3e6-50c9-4a36-a4c9-8da436042309"}]}}}}]},"TypeGuid":"(Guid)6f9dac99-8de1-4efc-8465-68ac443b7d08","EmbeddedTypeGuids":{"@Type":"[Guid]","@Value":["(Guid)a9ed5b7e-75c5-4651-af16-d2c27e98cb94","(Guid)3b83b776-fb25-43b8-99f2-3c507c9143fc"]}}},"TypeInfos":{"2c41fa04":"{2c41fa04-1834-41c1-816e-303c7aa2c05b}","81297157":"{81297157-7ec9-45ce-845e-84cab2b88ade}","829a18f2":"{829a18f2-c514-4f6e-9634-1df173429203}","fa2ee218":"{fa2ee218-a39b-4b6d-b249-49dbddbd168a}","Guid":"System.Guid","string":"System.String"}},"object":{"Graph":{"@Type":"6f9dac99","@Value":{"SpecialFunc":{"@Type":"0db3d7bb:Enum","@Value":"None"},"Implementation":{"@Type":"3b83b776","@Value":{"TextDocument":{"@Type":"f3878285","@Value":{"TextBlobForSerialisation":"(string)\n//  Turn our VFD enable output on or off depending on whether or not there's a request for the mixer to run:\n\n\n\n//  Convert our 0-100% to a 4-20 mA signal and send it to the correct word of the IO-link D/A converter\n\n// \n\nDP1213_IO_link.write_word_0 := TO_WORD(Sentronic_Real);\nDP1213_IO_link.write_word_1 := TO_WORD(Sentronic_Real);\n\n//    WARNINGS       WARNINGS     WARNINGS\n\n//   VFD not responding warning\n\nmixer_VFD_not_responding_warning (OK :=,\n//\tNOT VFD_enable\t\t\t\t\t\t\t\t\t\t\t//   We are OK if we either aren't trying to run the VFD,\n//\tOR VFD_feedback,\t\t\t\t\t\t\t\t\t\t//   or if we see a signal that the VFD is running.\n\twarning_message := mixer_VFD_not_responding_message,\n\ttime_delay:= 5);\n\n\n//  UNKNOWN DEVICE WARNING!!      This warning checks if the device ID of our digital to analog adapter (used to send the 4-20 mA speed signal) is an unfamiliar one.\n\nunknown_device_warning (OK:=\n\tDP1213_IO_link.not_connected\t\t\t\t//  Don't fault if nothing at all is connected (that's a different warning.)\n\tOR DP1213_IO_link.master_disconnected\t\t//  Don't fault if the master is disconnected (to avoid spamming the operator with secondary disconnects when the master disconnect is the real problem.)\n\tOR (DP1213_IO_link.device_ID = 612\t\t\t//  and of course we are OK if we do see the expected IFM DP1213 adapter.\n\tAND DP1213_IO_link.vendor_ID = 310),\n\twarning_message:= unknown_device_message,\n\ttime_delay := 10);\n\t\n\n//  DEVICE DISCONNECTED WARNING!!   This section checks if our DP1312 (D/A converter) is connected.\n\ndisconnected_adapter_warning (OK:=\n\tNOT DP1213_IO_link.not_connected\t\t\t//  We are OK if the master block isn't reporting a disconnect or communication problem.\n\tAND NOT DP1213_IO_link.data_invalid,\n\twarning_message:= disconnected_adapter_message,\n\ttime_delay := 10);\n\n\n//  DEVICE SHORT CIRCUIT WARNING!!\n\nshorted_device_warning (OK :=\n\tNOT DP1213_IO_link.short_circuit,\n\twarning_message := shorted_device_message);\n","LineInfoPersistence":"(string)24cef3d2-6105-4b2c-8400-9e6ff3f927cf_Impl_LineIds"}}}},"Interface":{"@Type":"a9ed5b7e","@Value":{"TextDocument":{"@Type":"f3878285","@Value":{"TextBlobForSerialisation":"(string)PROGRAM Feed_Diaphram_Pump\nVAR\n\thand_run_mixer: BOOL;\t\t\t\t\t//  True if somebody is pressing the button to hand run the mixer.\n\t\t//   WARNINGS\n\t\n\t//  VFD not responding Warning.\n\n\tmixer_VFD_not_responding_message: warning_message := (red := TRUE, pop_up_everywhere := TRUE, detail_warning_visu := 'W???', short_Description := 'Pit mixer VFD not responding');\n\t\n\tmixer_VFD_not_responding_warning: create_warning;\n\n\t//  Unknown device Warning\n\t\n\tunknown_device_message: warning_message := (red := TRUE, pop_up_everywhere := TRUE, detail_warning_visu := 'W???', short_Description := 'Unknown pit mixer adapter');\n\t\n\tunknown_device_warning: create_warning;\n\t\n\t//   Disconnected device Warning\n\n\tdisconnected_adapter_message: warning_message := (red := TRUE, pop_up_everywhere := TRUE, detail_warning_visu := 'W???',short_Description := 'Pit mixer adapter disconnected');\n\t\n\tdisconnected_adapter_warning: create_warning;\n\t\n\t//  Shorted device Warning\n\n\tshorted_device_message: warning_message := (red := TRUE, pop_up_everywhere := TRUE, detail_warning_visu := 'W???', short_Description := 'Pit mixer adapter shorted');\n\t\n\tshorted_device_warning: create_warning;\n\n\nEND_VAR\nVAR_INPUT\nSentronic_Real: REAL;\nEND_VAR\n\t\n\nVAR_IN_OUT\n\tDP1213_IO_link: IO_link_port;\nEND_VAR","LineInfoPersistence":"(string)24cef3d2-6105-4b2c-8400-9e6ff3f927cf_Decl_LineIds"}}}},"UniqueIdGenerator":"(string)553","POULevel":{"@Type":"8e575c5b:Enum","@Value":"Standard"},"ChildObjectGuids":{"@Type":"ArrayList:IList","@Value":[]},"AddAttributeSubsequent":"(bool)False"}},"TypeInfos":{"0db3d7bb":"{0db3d7bb-cde0-4416-9a7b-ce49a0124323}","3b83b776":"{3b83b776-fb25-43b8-99f2-3c507c9143fc}","6f9dac99":"{6f9dac99-8de1-4efc-8465-68ac443b7d08}","8e575c5b":"{8e575c5b-1d37-49c6-941b-5c0ec7874787}","a9ed5b7e":"{a9ed5b7e-75c5-4651-af16-d2c27e98cb94}","ArrayList":"System.Collections.ArrayList","bool":"System.Boolean","f3878285":"{f3878285-8e4f-490b-bb1b-9acbb7eb04db}","string":"System.String"}}},"FormatVersion":"1.0"}