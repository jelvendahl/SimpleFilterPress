{"payload":{"meta":{"Graph":{"@Type":"81297157","@Value":{"Guid":"(Guid)3da93330-1a68-4a58-9c11-535141447ea3","ParentGuid":"(Guid)fe54070b-3512-4a2f-b490-a57c15f4bfb5","Name":"(string)GVL","Properties":{"@Type":"2c41fa04:IDictionary","@Value":[{"@Key":"(Guid)829a18f2-c514-4f6e-9634-1df173429203","@Value":{"@Type":"829a18f2","@Value":{"ParentObjects":{"@Type":"fa2ee218:IDictionary","@Value":[{"@Key":"(Guid)d9b2b2cc-ea99-4c3b-aa42-1e5c49e65b84","@Value":"(Guid)a51f23ab-d24a-4bb2-824b-d597bc79ca33"}]}}}}]},"TypeGuid":"(Guid)ffbfa93a-b94d-45fc-a329-229860183b1d","EmbeddedTypeGuids":{"@Type":"[Guid]","@Value":["(Guid)a9ed5b7e-75c5-4651-af16-d2c27e98cb94"]}}},"TypeInfos":{"2c41fa04":"{2c41fa04-1834-41c1-816e-303c7aa2c05b}","81297157":"{81297157-7ec9-45ce-845e-84cab2b88ade}","829a18f2":"{829a18f2-c514-4f6e-9634-1df173429203}","fa2ee218":"{fa2ee218-a39b-4b6d-b249-49dbddbd168a}","Guid":"System.Guid","string":"System.String"}},"object":{"Graph":{"@Type":"ffbfa93a","@Value":{"Interface":{"@Type":"a9ed5b7e","@Value":{"TextDocument":{"@Type":"f3878285","@Value":{"TextBlobForSerialisation":"(string){attribute 'qualified_only'}\nVAR_GLOBAL\n\t\n\n\trunGreenHalo: BOOL := FALSE;  // shows (FALSE) or hides (TRUE) the green 'RUNNING' ring around the RUN/STOP button on the main screen.\n\tphYellowHalo: BOOL := FALSE;  // TRUE to show a yellow (minor fault) halo around the \"pH Control\" button on the main screen when there is a minor problem with the pH system\n\tphRedHalo: BOOL := FALSE;\t  // TRUE to show a red (critical fault) halo around the \"pH Control\" button on the main screen when there is a serious problem with the pH system\n\tsanitizerYellowHalo: BOOL := TRUE;  // FALSE to show a yellow (minor fault) halo around the \"Sanitizer\" button on the main screen when there is a minor problem with the bromine system\n\tsanitizerRedHalo: BOOL := TRUE;     // FALSE to show a red (critical fault) halo around the \"Sanitizer\" button on the main screen when there is a serious problem with the bromine system\n\n\tshow_change_time: BOOL;\t\t\t// True if we want to display the frame to change the time settings.\n\t\n\tWarningButtonPressed: INT;    // this tells the Warning system which button was pressed on the Warning Log screen\n\tWarningGlobalReset: BOOL := FALSE;   //  The reset button on the Warning Log screen toggles this to true, which signals Warnings (PRG)\n\twarningSecondaryReset: BOOL := FALSE;  //  This is the signal to the rest of the system to re-set all 'warning sent' flags.\n\tresettime: TIME;   //   When the initial warning reset button was pressed, lets the Warnings PRG decide when to stop broadcasting the reset signal.\n\tforcekeylightupdate: BOOL;  // gives us an extra way to trigger the update mechanism for key lights without actually changing the visu\n\tsiren: BOOL;   // Master switch to turn the audible siren of the stack light on and off.\n\t\n\t\n\nEND_VAR","LineInfoPersistence":"(string)3da93330-1a68-4a58-9c11-535141447ea3_Decl_LineIds"}}}},"NetVarProperties":null,"ParameterList":"(bool)False","AddAttributeSubsequent":"(bool)False"}},"TypeInfos":{"a9ed5b7e":"{a9ed5b7e-75c5-4651-af16-d2c27e98cb94}","bool":"System.Boolean","f3878285":"{f3878285-8e4f-490b-bb1b-9acbb7eb04db}","ffbfa93a":"{ffbfa93a-b94d-45fc-a329-229860183b1d}","string":"System.String"}}},"FormatVersion":"1.0"}